<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.marclazolatorre.betapp">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.marclazolatorre.betapp.Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.marclazolatorre.betapp.Activity.EventActivity"/>
        <activity android:name="com.example.marclazolatorre.betapp.Activity.MatchActivity"/>
        <activity android:name="com.example.marclazolatorre.betapp.Activity.BetsActivity"/>
        <activity android:name="com.example.marclazolatorre.betapp.Activity.BetActivity"/>
        <activity android:name="com.example.marclazolatorre.betapp.Activity.SportActivity"/>

    </application>

</manifest>